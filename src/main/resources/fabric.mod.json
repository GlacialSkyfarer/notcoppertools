{
  "schemaVersion": 1,
  "id": "notcoppertools",
  "version": "${version}",

  "name": "It's Not Copper Tools",
  "description": "A mod that adds things that aren't copper tools.",
  "authors": [
    "GlacialSkyfarer"
  ],
  "contact": {
    "homepage": "https://glass-launcher.net/",
    "sources": "https://github.com/calmilamsy/stationapi-example-mod"
  },

  "license": "CC0-1.0",
  "icon": "assets/notcoppertools/icon.png",

  "environment": "*",
  "entrypoints": {
    "stationapi:event_bus": [
      "io.github.GlacialSkyfarer.notcoppertools.events.init.CommonListener"
    ],
    "stationapi:event_bus_client": [
      "io.github.GlacialSkyfarer.notcoppertools.events.init.ClientListener"
    ],
    "stationapi:event_bus_server": [
      "io.github.GlacialSkyfarer.notcoppertools.events.init.ServerListener"
    ],
    "gcapi3": [
      "io.github.GlacialSkyfarer.notcoppertools.NotCopperTools"
    ]
  },
  "accessWidener": "notcoppertools.accesswidener",
  "mixins": [
    "notcoppertools.mixins.json"
  ],

  "depends": {
    "minecraft": "1.0.0-beta.7.3"
  }

}
