{
    "multipart": [
        {   "when": {
            "north": "false",
            "east": "false",
            "west": "false",
            "south": "false"
        },
            "apply": { "model": "notcoppertools:block/iron_grating_post" }
        },
        {   "when": { "north": "true" },
            "apply": { "model": "notcoppertools:block/iron_grating_side", "uvlock": true }
        },
        {   "when": { "east": "true" },
            "apply": { "model": "notcoppertools:block/iron_grating_side", "y": 90, "uvlock": true }
        },
        {   "when": { "south": "true" },
            "apply": { "model": "notcoppertools:block/iron_grating_side", "y": 180, "uvlock": true }
        },
        {   "when": { "west": "true" },
            "apply": { "model": "notcoppertools:block/iron_grating_side", "y": 270, "uvlock": true }
        }
    ]
}
